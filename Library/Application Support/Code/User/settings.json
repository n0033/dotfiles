{
  "git.autofetch": true,
  "autoDocstring.docstringFormat": "sphinx",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.parameterHints.enabled": false,
  "json.maxItemsComputed": 1000000,
  "editor.largeFileOptimizations": false,
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  },
  "git.enableCommitSigning": true,
  "editor.tabSize": 2,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": true
  },
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "editor.lineNumbers": "relative",
  "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/init.vim",
  "files.associations": {
    ".env": "shellscript"
  },
  "files.insertFinalNewline": true,
  "editor.formatOnSave": true,
  "python.formatting.yapfArgs": [
    "--style",
    "yapf"
  ],
  "python.formatting.provider": "yapf",
  "editor.detectIndentation": false,
  "haskell.manageHLS": "GHCup",
  "haskell.ghcupExecutablePath": "/Users/norbertwyszkowski/.ghcup/bin/ghcup",
  "security.workspace.trust.untrustedFiles": "open",
  "redhat.telemetry.enabled": false,
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "gitlens.advanced.messages": {
    "suppressCommitNotFoundWarning": true
  },
  "haskell.checkProject": false,
  "[python]": {
    "editor.formatOnType": true
  },
  "notebook.lineNumbers": "on",
  "go.toolsManagement.autoUpdate": true
}
